{
  "name": "loveserenespaces",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma migrate deploy && npx prisma generate --no-engine && next build",
    "start": "npx prisma migrate deploy && npx prisma generate --no-engine && next start -p 3000",
    "build:analyze": "ANALYZE=true npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "postinstall": "npx prisma generate --no-engine"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.14.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@stripe/stripe-js": "^7.8.0",
    "@types/nodemailer": "^7.0.1",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.2",
    "googleapis": "^156.0.0",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "prisma": "^6.14.0",
    "puppeteer": "^24.22.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "stripe": "^18.4.0",
    "swr": "^2.3.6",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
